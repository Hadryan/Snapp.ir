    script.
        $(function () {
            $('.pagination li a').on('click', function (e) {
                e.preventDefault()
                pageIndex = $(this).data('page-index')
                $('#searchForm #page').val(pageIndex)
                $('#searchForm').submit()
            })
            $('#searchBtn').on('click', function (e) {
                e.preventDefault()
                $('#searchFlag').val(1)
                $('#searchForm').submit()
            })
            $('#searchForm input').keydown(function (e) {
                if (e.keyCode == 13) {
                    e.preventDefault()
                    $('#searchFlag').val(1)
                    $('#searchForm').submit()
                }
            })
            checkSort()
        });
        function showNewPageSize(id) {
            $('#pageSize').val(id);
            $('#searchFlag').val(1);
            $('#searchForm').submit();
        }

        function sortData(field, condition) {
            $('#sortField').val(field);
            $('#sortCondition').val(condition);
            $('#searchForm').submit();
        }

        function checkSort() {
            if ($('#sortField').length) {
                var sortField = $('#sortField').val();
                var sortCondition = $('#sortCondition').val();
                if (sortField != '') {
                    var icon = sortCondition == 'ASC' ? '<span class=\'fa fa-sort-asc fa-lg\'></span>' : '<span class=\'fa fa-sort-desc fa-lg\'></span>';
                    var newSortCondition = sortCondition == 'ASC' ? 'DESC' : 'ASC';
                    $('.sortable-' + sortField).append(icon);
                    $('.sortable-' + sortField).attr('onclick', 'sortData(\'' + sortField + '\',\'' + newSortCondition + '\');');
                }
            }
        }
        $('.bigShow').on('click', function () {
            var image = $(this).children()[0].src
            $("#imagepreview").attr('src', image)
            $('#imagemodal').modal('show');
            var expire_date = $(this).attr('date');
            if (expire_date && expire_date != "") {
                $("#expire").val(expire_date)
            } else {
                $("#expire").val("-")
            }
        })

        $('.const').on('click', function (event) {
            event.preventDefault()
            window.location = '/backend/not_found'
        })

        moment.loadPersian();

        var cost = "";
        function separate_nums(nums) {
            if (nums == 'null') {
                return nums;
            }

            if (nums.length > 3) {
                cost = "," + nums.substr(-3) + cost;
                separate_nums(nums.substr(0, nums.length - 3));
                return cost;
            } else {
                cost = nums + cost;
                return cost;
            }
        }

        function validateForm(formId) {
            var $hasError = false;

            var formData = $('#' + formId).serializeJSON();
            console.log(formData);

            // do validation
            $.each(formData, function ($index, $elmId) {
                console.log($index, $elmId);
                var $elm = $('#' + $elmId);
                var $prnt = $('#' + $elm.name).parent();
                if ($prnt.hasClass('has-success')) {
                    $prnt.removeClass('has-success');
                }
                if ($prnt.hasClass('has-error')) {
                    $prnt.removeClass('has-error');
                }
                if ($('#' + $elm.name).data('req') == true && $elm.value == '') {
                    $prnt.addClass('has-error');
                    $hasError = true;
                } else {
                    //$prnt.addClass('has-success');
                }
            });

            if ($hasError) {
                frmShowError({'ALL': 'لطفا تمام فیلد ها را با دقت تکمیل نمایید'})
            }
            return !$hasError;
        }

        function frmShowError(errObj) {
            notyfy({
                force: true,
                timeout: 3000,
                text: errObj.ALL,
                type: 'error',
                layout: 'top',
                closeWith: ['click']
            });
        }


        function frmShowSuccess(successObj) {
            notyfy({
                force: true,
                timeout: 3000,
                text: successObj.ALL,
                type: 'success',
                layout: 'top',
                closeWith: ['click']
            });
        }

        // format geregorian to jalali date
        function g2j(gDate) {
            return moment(gDate).format('jYYYY/jMM/jDD'); // accept format LT, LL, LLL, LLLL
        }

        function g2j2(gDate) {
            Date.prototype.addHours = function (h) {
                this.setTime(this.getTime() + (h * 60 * 60 * 1000));
                return this;
            }
            //var gDate=new Date(gDate).addHours(3.5)
            return moment(gDate).format('jYYYY/jMM/jDD HH:mm'); // accept format LT, LL, LLL, LLLL
        }

        function g2Time(gDate) {
            Date.prototype.addHours = function (h) {
                this.setTime(this.getTime() + (h * 60 * 60 * 1000));
                return this;
            }
            //var gDate=new Date(gDate).addHours(3.5)
            return moment(gDate).format('HH:mm'); // accept format LT, LL, LLL, LLLL
        }

